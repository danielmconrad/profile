#!/bin/bash

# Git
alias ormaster="git stash && git fetch origin && git checkout master && git reset --hard origin/master && git remote prune origin"
alias upmaster="git stash && git fetch upstream && git checkout master && git reset --hard upstream/master && git remote prune upstream && git remote prune origin"

